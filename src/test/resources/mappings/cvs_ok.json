{
  "request": {
    "url": "/mock-api/cvs/value",
    "method": "POST",
    "bodyPatterns": [
      { "matchesJsonPath": "$[?(@.weight =~ /[0-2000]/)]" },
      { "matchesJsonPath": "$[?(@.age =~ /[0-40]/)]" },
      { "matchesJsonPath": "$[?(@.humps =~ /[0-3]/)]" },
      { "matchesJsonPath": "$[?(@.condition =~ /[1-9]/)]" },
      { "matchesJsonPath": "$[?(@.rating =~ /[1-5]/)]" }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "value": 42
    }
  }
}
